<!DOCTYPE HTML>

<%= form_for @event, url: {action: "create"} do |f| %>
  Title in 30 characters or less<br><%= f.text_field :title %>
  <br>Description in 250 characters or less (make sure to leave date and time!)<br><%= f.text_area :description, size: "60x3"%>
  <br><br><%= f.submit "Create" %>
<% end %>
